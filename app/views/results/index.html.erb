<p id="notice"><%= notice %></p>

<h1>Results</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Cincuenta mts</th>
      <th>Cien mts</th>
      <th>Doscientos mts</th>
      <th>Cuatrocientos mts</th>
      <th>Ochocientos mts</th>
      <th>Mil quinientos mts</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |result| %>
      <tr>
        <td><%= result.date %></td>
        <td><%= result.cincuenta_mts %></td>
        <td><%= result.cien_mts %></td>
        <td><%= result.doscientos_mts %></td>
        <td><%= result.cuatrocientos_mts %></td>
        <td><%= result.ochocientos_mts %></td>
        <td><%= result.mil_quinientos_mts %></td>
        <td><%= result.user %></td>
        <td><%= link_to 'Show', result %></td>
        <td><%= link_to 'Edit', edit_result_path(result) %></td>
        <td><%= link_to 'Destroy', result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Result', new_result_path %>
